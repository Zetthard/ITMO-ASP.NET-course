@{
    ViewBag.Title = "Students";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>London is the capital of Great Britain.</p>

<div class="col-md-1">
    <h3>Студенты</h3>
    <table class="table-bordered" cellpadding="5">
        <tr>
            <td><h4>Имя студента</h4></td>
        </tr>
        @foreach (var student in ViewBag.Students)
        {
            <tr>
                <td><p>@student</p></td>
            </tr>
        }
    </table>
    <br />
    <p><a class="btn btn-primary btn-default" href="/Home/AddStudent">Add new student&raquo;</a></p>
</div>
