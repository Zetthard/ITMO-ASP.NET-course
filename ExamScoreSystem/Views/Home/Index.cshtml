@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Exam App</h1>
    <p class="lead">This App is for tracking students exams.</p>
</div>


<div class="row">
    <div class="col-md-4">
        <h3>Студенты</h3>
        <table class="table-bordered">
            <tr>
                <td><h4>Имя студента</h4></td>
            </tr>
            @foreach (var student in ViewBag.Students)
            {
                <tr>
                    <td>@student</td>
                </tr>
            }
        </table>
    </div>
    <div class="col-md-4">
        <h3>Курсы</h3>
        <table class="table-bordered">
            <tr>
                <td><h4>Название курса</h4></td>
            </tr>
            @foreach (var course in ViewBag.Courses)
            {
                <tr>
                    <td>@course</td>
                </tr>
            }
        </table>
    </div>
</div>
<a></a>