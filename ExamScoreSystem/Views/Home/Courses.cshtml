@model IEnumerable<ExamScoreSystem.Models.Course>

@{
    ViewBag.Title = "Courses";
}
<h2>@ViewBag.Title.</h2>

<div class="col-md-1">
    <table class="table-bordered" cellpadding="5">
        <tr>
            <td><h4>Название курса</h4></td>
        </tr>
        @foreach (var course in Model)
        {
            <tr>
                <td><a href="/Home/EnterExam">@course</a></td>
            </tr>
        }
    </table>
</div>